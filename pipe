pipeline {
    agent any

    stages {
        stage('bhaiyu ki den') {
            steps {
                git branch: 'main', url: 'https://github.com/lekhraj99/lekhraj.git'
            }
        }
		stage('install docker') {
            steps {
                sh'''
				sudo apt-get install -y docker
				sudo systemctl start docker 
				sudo systemctl enable docker 
				'''
            }
        } stages {
        stage('bhaiyu') {
            steps {
                echo'subham rathore kese ho'
            }
        }
		
            }
        }
		
    }
}
